{
  "name": "backend-nodejs-s3",
  "version": "1.0.0",
  "author": "Kent Louise Tonino",
  "scripts": {
    "build": "tsc --build",
    "start": "tsc --build && node ./dist/server.js",
    "dev": "nodemon --config nodemon.json ./src/server.ts",
    "format": "prettier --write \"./**/*.{html,css,ts,js,json,prettierrc,yml}\"",
    "test:all": "jest --watchAll --verbose --coverage --runInBand --no-cache"
  },
  "dependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.5.4",
    "@types/morgan": "^1.9.5",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.5.7",
    "@types/nodemon": "^1.19.2",
    "@types/prettier": "^3.0.0",
    "@types/superagent": "^4.1.18",
    "@types/supertest": "^2.0.12",
    "aws-sdk": "^2.1448.0",
    "babel-cli": "^6.26.0",
    "babel-jest": "^29.6.4",
    "babel-preset-env": "^1.7.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.1",
    "jest": "^29.6.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "superagent": "^8.1.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "testEnvironment": "node",
    "modulePathIgnorePatterns": [
      "<rootDir>/coverage/"
    ]
  }
}
